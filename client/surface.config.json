{
    "context":  "source",
    "entry":    "app/index.ts",
    "filename": "[name]/[hash].js",
    "tsconfig": "tsconfig.json",
    "tslint":   "../tslint.json",
    "output":   "../server/wwwroot",
    "htmlTemplate":
    {
        "filename": "[name]/index.html",
        "template": "template.html"
    },
    "simblingResolve":
    {
        "from":    ".js",
        "to":      ".ts",
        "include": ["node_modules/@surface", "../modules/source/@surface"]
    }
}