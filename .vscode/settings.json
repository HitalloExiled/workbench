{
    "breadcrumbs.enabled":     true,
    "editor.roundedSelection": false,
    "files.associations":
    {
        "*.config": "xml",
        "*.resx":   "xml"
    },
    "files.exclude":
    {
        "**/.DS_Store":   true,
        "**/.git":        true,
        "**/.github":     true,
        "**/.gitignore":  true,
        "**/.gitmodules": true,
        "**/.hg":         true,
        "**/.nyc_output": true,
        "**/.nycrc":      true,
        "**/.svn":        true,
        "**/.vs":         true,
        "**/*.config":    true,
        "**/*.d.ts":
        {
            "when": "$(basename).js"
        },
        "**/*.js":
        {
            "when": "$(basename).ts"
        },
        "**/*.js.map":          true,
        "**/coverage":          true,
        "**/CVS":               true,
        "**/LICENSE":           true,
        "**/node_modules":      true,
        "**/package-lock.json": true,
        "**/package.json":      false,
        "**/README.md":         true,
        "**/tsconfig*.json":    false,
        "**/tslint.json":       false
    },
    "highlight.maxMatches": 500,
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.format.placeOpenBraceOnNewLineForFunctions":     true,
    "javascript.implicitProjectConfig.checkJs":                  true,
    "javascript.preferences.quoteStyle":                         "double",
    "sync.autoDownload":                                         false,
    "sync.autoUpload":                                           false,
    "sync.forceDownload":                                        false,
    "sync.gist":                                                 "1296ff92e87dad728b76adfd5b8d0c62",
    "sync.quietSync": false,
    "sync.removeExtensions":                                     true,
    "sync.syncExtensions":                                       true,
    "typescript.tsdk":                                           "modules/source/@surface/compiler/node_modules/typescript/lib",
    "vsicons.associations.folders":
    [
        { "extensions": ["internal"], "icon": "private" },
        { "extensions": ["tasks"],    "icon": "tools" },
    ],
    "workbench.iconTheme": "vscode-icons"
}