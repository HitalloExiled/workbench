{
    "breadcrumbs.enabled":     true,
    "editor.roundedSelection": false,
    "files.exclude":
    {
        "**/*.config":          true,
        "**/.git":              true,
        "**/.github":           true,
        "**/.gitignore":        true,
        "**/.gitmodules":       true,
        "**/.hg":               true,
        "**/.nycrc":            true,
        "**/.nyc_output":       true,
        "**/.svn":              true,
        "**/.vs":               true,
        "**/.DS_Store":         true,
        "**/coverage":          true,
        "**/node_modules":      true,
        "**/package.json":      false,
        "**/package-lock.json": true,
        "**/tslint.json":       false,
        "**/tsconfig*.json":    false,
        "**/CVS":               true,
        "**/LICENSE":           true,
        "**/README.md":         true,
        "**/*.js":              { "when": "$(basename).ts" },
        "**/*.d.ts":            { "when": "$(basename).js" },
        "**/*.js.map":          true,
    },
    "files.associations":
    {
        "*.config": "xml",
        "*.resx":   "xml"
    },
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.format.placeOpenBraceOnNewLineForFunctions":     true,
    "javascript.implicitProjectConfig.checkJs":                  true,
    "javascript.preferences.quoteStyle":                         "double",
    "sync.autoDownload":                                         false,
    "sync.autoUpload":                                           false,
    "sync.forceDownload":                                        false,
    "sync.gist":                                                 "1296ff92e87dad728b76adfd5b8d0c62",
    "sync.quietSync":                                            false,
    "sync.removeExtensions":                                     true,
    "sync.syncExtensions":                                       true,
    "typescript.tsdk":                                           "client/node_modules/typescript/lib",
    "workbench.iconTheme":                                       "vscode-icons",
    "highlight.maxMatches":                                      500,
    "highlight.regexes":
    {
        "(#(?:for|if|else-if) *= *\")([^\"]*)(\")":
        {
            "decorations":
            [
                { "color": "" },
                { "color": "violet" },
                { "color": "" },
            ],
            "filterFileRegex":      ".*\\.html",
            "highlight.regexFlags": "gm"
        },
        "(<)(/?)(\\w+-(-|\\w)+)":
        {
            "decorations":
            [
                { "color": "" },
                { "color": "" },
                { "color": "#9688de" },
            ],
            "filterFileRegex":      ".*\\.html",
            "highlight.regexFlags": "gm"
        },
        "(\\[\\[)(.+?)(\\]\\])":
        {
            "decorations":
            [
                { "color": "lightgray" },
                { "color": "violet" },
                { "color": "lightgray" },
            ],
            "filterFileRegex":      ".*\\.html",
            "highlight.regexFlags": "gm"
        },
        "(\\{\\{)(.+?)(\\}\\})":
        {
            "decorations":
            [
                { "color": "lightgray" },
                { "color": "violet" },
                { "color": "lightgray" },
            ],
            "filterFileRegex":      ".*\\.html",
            "highlight.regexFlags": "gm"
        },
    }
}